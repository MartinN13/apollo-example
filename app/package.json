{
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "NODE_ENV=production rm -rf dist/* && webpack --mode production --env.production",
    "dev": "NODE_ENV=development webpack --mode development --env.production=false && webpack-dev-server --env.production=false",
    "serve": "cd dist && python3 -c \"from http.server import test, SimpleHTTPRequestHandler as RH; RH.extensions_map={k:v+';charset=UTF-8' for k,v in RH.extensions_map.items()}; test(RH, port=8080)\"",
    "fix": "tsc && eslint 'src/**/*.{js,ts,tsx}' --ignore-path .gitignore --quiet --fix"
  },
